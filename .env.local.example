# Database (Postgres)
DATABASE_URL=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# Stripe Payments
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_PRICE_MONTHLY=price_...
STRIPE_PRICE_YEARLY=price_...

# Error Monitoring (Sentry)
SENTRY_DSN=https://...@sentry.io/...
NEXT_PUBLIC_SENTRY_DSN=https://...@sentry.io/...

# Alert routing (set at least one in production)
SENTRY_ALERT_WEBHOOK_URL=https://hooks.example.com/fastlane-alerts
ALERT_EMAIL_TO=oncall@example.com

# Security
CRON_SECRET=replace-with-32-char-minimum-secret-abcdef
USER_COOKIE_SECRET=replace-with-32-char-minimum-secret-abcdef
FASTLANE_ACCOUNT_SESSION_SECRET=replace-with-32-char-minimum-secret-abcdef
FASTLANE_LOGIN_TOKEN_SECRET=replace-with-32-char-minimum-secret-abcdef

# Auth Email Delivery (required in production for token login)
RESEND_API_KEY=re_...
FASTLANE_AUTH_EMAIL_FROM=FastLane <no-reply@example.com>
FASTLANE_AUTH_EMAIL_REPLY_TO=support@example.com

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
